{
	"name": "app",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --port 8080",
		"start": "vite --port 8080",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"build:tailwind": "tailwind build static/assets/styles/index.css -o static/assets/styles/tailwind.css",
		"build:fontawesome": "mkdir -p static/assets/vendor/@fortawesome/fontawesome-free/webfonts && mkdir -p static/assets/vendor/@fortawesome/fontawesome-free/css && cp -a ./node_modules/@fortawesome/fontawesome-free/webfonts static/assets/vendor/@fortawesome/fontawesome-free/ && cp ./node_modules/@fortawesome/fontawesome-free/css/all.min.css static/assets/vendor/@fortawesome/fontawesome-free/css/all.min.css",
		"db:generate": "drizzle-kit generate",
		"db:push": "drizzle-kit push",
		"db:pull": "drizzle-kit introspect:sqlite",
		"db:drop": "drizzle-kit drop",
		"db:check": "drizzle-kit check:sqlite",
		"db:studio": "drizzle-kit studio"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-node": "^5.0.1",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/better-sqlite3": "^7.6.9",
		"@types/eslint": "^8.56.0",
		"@types/node": "^20.12.12",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"drizzle-kit": "^0.21.4",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"postcss": "8.4.38",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"sveltekit-flash-message": "^2.4.4",
		"sveltekit-superforms": "^1.13.4",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3",
		"zod": "^3.22.4"
	},
	"dependencies": {
		"@fortawesome/fontawesome-free": "5.15.3",
		"@godaddy/terminus": "^4.12.1",
		"@lucia-auth/adapter-drizzle": "^1.0.1",
		"@popperjs/core": "2.9.1",
		"@rollup/plugin-alias": "3.1.2",
		"@tailwindcss/forms": "0.2.1",
		"better-sqlite3": "^9.4.1",
		"bits-ui": "^0.21.10",
		"chart.js": "2.9.4",
		"clsx": "^2.1.1",
		"dotenv": "^16.4.5",
		"drizzle-orm": "^0.29.3",
		"drizzle-zod": "^0.5.1",
		"express": "^4.19.2",
		"fast-xml-parser": "^4.4.0",
		"helmet": "^7.1.0",
		"lucia": "^3.2.0",
		"lucide-svelte": "^0.379.0",
		"oslo": "^1.2.0",
		"sirv-cli": "1.0.11",
		"svelte-meta-tags": "^3.1.2",
		"svelte-routing": "^2.13.0",
		"svelte-sonner": "^0.3.24",
		"tailwind-merge": "^2.3.0",
		"tailwind-variants": "^0.2.1",
		"tailwindcss": "2.0.4"
	},
	"type": "module"
}
